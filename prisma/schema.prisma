generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")  
}

model Room {
  id         String       @id @default(uuid())
  name       String
}

model SensorData {
  id          String   @id @default(uuid())
  temperature Float
  humidity    Float
  timestamp   DateTime @default(now())
  roomId      String
}
